<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
		android:id="@+id/cardView"
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" style="@style/CardView.Light"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="8dp"
		android:layout_marginStart="8dp"
		android:layout_marginEnd="8dp">

	<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="395dp"
			android:layout_height="80dp">

		<TextView
				android:text="TextView"
				android:id="@+id/sub_text2"
				android:layout_width="0dp"
				android:layout_height="19dp"
				android:textAppearance="@style/TextAppearance.AppCompat.Body1"
				android:textColor="#8A000000"
				android:layout_marginTop="16dp"
				app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/media_image"
				app:layout_constraintEnd_toStartOf="@+id/media_image3" android:layout_marginEnd="19dp"
				android:layout_marginRight="19dp"/>
		<TextView
				android:text="TextView"
				android:id="@+id/sub_text"
				android:layout_width="0dp"
				android:layout_height="19dp"
				android:textAppearance="@style/TextAppearance.AppCompat.Body1"
				android:textColor="#8A000000"
				android:layout_marginBottom="16dp"
				app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/sub_text2"
				app:layout_constraintEnd_toEndOf="@+id/sub_text2"/>
		<ImageView
				android:id="@+id/media_image"
				android:layout_width="0dp"
				android:layout_height="48dp"
				tools:ignore="ContentDescription"
				android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
				android:layout_marginStart="16dp" app:layout_constraintEnd_toStartOf="@+id/sub_text2"
				android:layout_marginEnd="20dp" android:layout_marginRight="20dp"
				app:srcCompat="@android:drawable/checkbox_off_background"/>
		<ImageView
				android:id="@+id/media_image2"
				android:layout_width="0dp"
				android:layout_height="0dp"
				tools:ignore="ContentDescription"
				android:layout_marginTop="17dp" app:layout_constraintEnd_toStartOf="@+id/textView2"
				android:layout_marginBottom="9dp" app:layout_constraintBottom_toTopOf="@+id/media_image3"
				app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/media_image3"
				app:srcCompat="@drawable/ic_death"/>
		<ImageView
				android:id="@+id/media_image3"
				android:layout_width="0dp"
				android:layout_height="0dp"
				tools:ignore="ContentDescription"
				android:layout_marginBottom="16dp"
				app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/media_image2"
				app:layout_constraintStart_toEndOf="@+id/sub_text2"
				app:layout_constraintEnd_toStartOf="@+id/textView3" app:srcCompat="@drawable/ic_death"/>
		<TextView
				android:text="x0"
				android:layout_width="0dp"
				android:layout_height="0dp"
				android:id="@+id/textView2"
				android:layout_marginTop="17dp"
				app:layout_constraintStart_toEndOf="@+id/media_image3" android:layout_marginBottom="9dp"
				app:layout_constraintEnd_toEndOf="@+id/textView3" app:layout_constraintBottom_toTopOf="@+id/textView3"
				app:layout_constraintTop_toTopOf="parent"/>
		<TextView
				android:text="x0"
				android:layout_width="0dp"
				android:layout_height="0dp"
				android:id="@+id/textView3"
				android:layout_marginBottom="16dp"
				app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView2"
				app:layout_constraintStart_toEndOf="@+id/media_image3" app:layout_constraintEnd_toEndOf="parent"
				android:layout_marginEnd="59dp" android:layout_marginRight="59dp"/>

	</androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>