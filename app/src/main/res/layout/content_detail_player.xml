<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"

		tools:showIn="@layout/activity_detail_player"
		tools:context=".DetailPlayerActivity">
	<TextView
			android:text="@string/label_main_weapon"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView_main_weapon"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"/>
	<Spinner
			android:layout_width="205dp"
			android:layout_height="40dp"
			android:id="@+id/spinner_main_guns"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_main_weapon" app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="53dp"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_new"
			android:id="@+id/imageButton_add_main_gun"
			android:background="@null"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="72dp" android:layout_marginRight="72dp"
			app:layout_constraintStart_toStartOf="@+id/spinner_main_guns" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_add_weapon" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_main_weapon" app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="51dp"
			android:background="@null"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_delete"
			android:id="@+id/imageButton_delete_main_gun"
			app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_remove_weapon" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_main_weapon" app:layout_constraintVertical_bias="0.0"/>
	<TextView
			android:text="@string/label_casualties"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView_main_casualties"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/spinner_main_guns" app:layout_constraintVertical_bias="0.0"/>
	<EditText
			android:layout_width="161dp"
			android:layout_height="48dp"
			android:inputType="none"
			android:ems="10"
			android:id="@+id/editText_main_casualties"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
			android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
			app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:autofillHints="@string/label_casualties" tools:targetApi="o" tools:ignore="LabelFor"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView_main_casualties"
			app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="53dp"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_remove"
			android:id="@+id/imageButton_remove_main_casualty"
			android:background="@null"
			app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
			android:layout_marginRight="8dp"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_add_casualty"
			app:layout_constraintStart_toEndOf="@+id/editText_main_casualties" android:layout_marginStart="8dp"
			android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_main_casualties"
			app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="53dp"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_add"
			android:id="@+id/imageButton_add_main_casualty"
			android:background="@null"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_remove_casualty"
			app:layout_constraintStart_toEndOf="@+id/imageButton_remove_main_casualty"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView_main_casualties"
			app:layout_constraintVertical_bias="0.0"/>
	<View
			android:id="@+id/divider_1"
			android:layout_width="395dp"
			android:layout_height="1dp"
			android:background="?android:attr/listDivider"
			android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/imageButton_remove_main_casualty"
			app:layout_constraintVertical_bias="0.0"/>
	<TextView
			android:text="@string/label_secondary_weapon"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView_secondary_weapon"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/divider_1" app:layout_constraintVertical_bias="0.0"/>
	<Spinner
			android:layout_width="204dp"
			android:layout_height="40dp"
			android:id="@+id/spinner_secondary_guns"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.005"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_secondary_weapon"
			app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="53dp"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_new"
			android:id="@+id/imageButton_add_secondary_gun"
			android:background="@null"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="72dp" android:layout_marginRight="72dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_add_weapon"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView_secondary_weapon"
			app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="51dp"
			android:background="@null"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_delete"
			android:id="@+id/imageButton_delete_secondary_gun"
			app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_remove_weapon"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView_secondary_weapon"
			app:layout_constraintVertical_bias="0.0"/>
	<TextView
			android:text="@string/label_casualties"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView_secondary_casualties"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/spinner_secondary_guns" app:layout_constraintVertical_bias="0.0"/>
	<EditText
			android:layout_width="155dp"
			android:layout_height="48dp"
			android:inputType="none"
			android:ems="10"
			android:id="@+id/editText_secondary_casualties"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
			android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
			android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:autofillHints="@string/label_casualties" tools:targetApi="o" tools:ignore="LabelFor"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView_secondary_casualties"
			app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="53dp"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_remove"
			android:id="@+id/imageButton_remove_secondary_casualty"
			android:background="@null"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_remove_casualty"
			app:layout_constraintStart_toEndOf="@+id/editText_secondary_casualties" android:layout_marginStart="8dp"
			android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.005" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_secondary_casualties"
			app:layout_constraintVertical_bias="0.0"/>
	<ImageButton
			android:layout_width="53dp"
			android:layout_height="50dp" app:srcCompat="@drawable/ic_add"
			android:id="@+id/imageButton_add_secondary_casualty"
			android:background="@null"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:contentDescription="@string/content_description_add_casualty"
			app:layout_constraintStart_toEndOf="@+id/imageButton_remove_secondary_casualty"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView_secondary_casualties"
			app:layout_constraintVertical_bias="0.0"/>
	<View
			android:id="@+id/divider2"
			android:layout_width="395dp"
			android:layout_height="1dp"
			android:background="?android:attr/listDivider"
			android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/imageButton_remove_secondary_casualty"
			app:layout_constraintVertical_bias="0.0"/>
	<TextView
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:id="@+id/textView_vest"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
			android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
			android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@+id/divider2"/>
	<Switch
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/switch_vest"
			android:switchMinWidth="100dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/divider2"/>
	<TextView
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:id="@+id/textView_helmet"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
			android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
			android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_vest" app:layout_constraintVertical_bias="0.037"/>
	<Switch
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/switch_helmet"
			android:switchMinWidth="100dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginTop="16dp"
			app:layout_constraintTop_toBottomOf="@+id/switch_vest"/>
	<TextView
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:id="@+id/textView_defuse_kit" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="16dp"
			android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
			android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@+id/textView_helmet" app:layout_constraintVertical_bias="0.043"/>
	<Switch
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center_vertical"
			android:switchMinWidth="100dp"
			android:id="@+id/switch_defuse_kit"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
			android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
			android:layout_marginTop="16dp"
			app:layout_constraintTop_toBottomOf="@+id/switch_helmet"/>
</androidx.constraintlayout.widget.ConstraintLayout>